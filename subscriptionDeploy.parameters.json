{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "rgName": {
            "type": "string",
            "defaultValue": "Demo-RG"
        },
        "rgLocation": {
            "type": "string",
            "defaultValue": "centralus"
        },
        "storagePrefix": {
            "type": "string",
            "maxLength": 11,
            "defaultValue": "demostore"
        },
        "vnetName": {
            "type": "string",
            "defaultValue": "VNet1"
        },
        "vmName": {
            "type": "string",
            "defaultValue": "DemoVM"
        },
        "adminUsername": {
            "type": "string",
            "defaultValue": "devADMuser"
        },
        "adminPassword": {
            "type": "securestring"
        }
    }
  }